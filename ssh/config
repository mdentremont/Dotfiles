# Configuration to connect to Spin-managed development environments.
# Important: leave this at the top of this SSH configuration file.
Include ~/.spin/ssh/include
Include ~/.config/spin/ssh/include

Host dnd-github
    Hostname github.com
    User git
    IdentityFile ~/.ssh/keys/dnd-github

Host github-other
    Hostname github.com
    User git
    IdentityFile ~/.ssh/keys/github-other
