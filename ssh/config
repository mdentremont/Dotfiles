Host nas
    HostName 192.168.2.50
    User root

Host pi
    HostName mattdentremont.ddns.net
    User root
    Port 22

Host pi-lan
    HostName 192.168.2.52
    User root

Host cubie-lan
    HostName 192.168.2.51
    Port 22222
    User mdentremont

Host cubie
    ProxyCommand ssh -q pi nc 192.168.2.51 22222
    User mdentremont


# Share a connection to each host, and keep it open for 10 minutes after last session closes
Host *
    User mdentremont
    ControlPath /tmp/sshmaster-%r@%h:%p
    ControlMaster auto
    ControlPersist 600
