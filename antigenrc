antigen use oh-my-zsh

# oh-my-zsh plugins

# Only enable plugin if virtualenvwrapper.sh exists
if hash virtualenvwrapper.sh; then
    antigen bundle virtualenvwrapper
fi

# Only enable git plugin if git exists
if hash git 2>/dev/null; then
    antigen bundle git
fi

# Only enable pip plugin if git exists
if hash pip 2>/dev/null; then
    antigen bundle pip
fi

antigen bundle vi-mode

# Non oh-my-zsh plugins
antigen bundle v17al/zsh-vim-mode
antigen bundle zsh-users/zsh-history-substring-search
antigen bundle zsh-users/zsh-syntax-highlighting

# Platform dependant bundles
if [[ `uname` == 'Linux' ]]
then
    antigen bundle command-not-found
elif [[ `uname` == 'Darwin' ]]
then
    # Only enable brew plugin if brew exists
    if hash brew 2>/dev/null; then
        antigen bundle brew
    fi
fi

# Non oh-my-zsh theme
antigen theme v17al/girazz

antigen apply

